<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.memo.post.mapper.PostMapper">

	<select id="selectPostListTest" resultType="map">
		SELECT
			*
		FROM
			`post`
	</select>
	
	<select id="getPostListByUserId" parameterType="int" resultType="com.memo.post.domain.Post">
		SELECT
			`id`
			,`userId`
			,`content`
			,`imagePath`
			,`createdAt`
			,`updatedAt`
		FROM
			`post`
		WHERE	
			`userId` = #{userId}	
	</select>
	
	<select id="savePostMapperByLoginId" resultType="map">
		SELECT
			*
	</select>
</mapper>